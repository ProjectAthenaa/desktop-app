{
  "name": "desktop-app",
  "productName": "desktop-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": {
    "name": "Andrew Bellucci",
    "email": "belludevelopment@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "desktop_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/main/index.html",
                  "js": "./src/renderer/main/renderer.ts",
                  "name": "main_window"
                },
                {
                  "html": "./src/renderer/auth/index.html",
                  "js": "./src/renderer/auth/renderer.ts",
                  "name": "auth_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@electron-forge/plugin-webpack": "6.0.0-beta.57",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-loader": "^8.2.2",
    "base64-inline-loader": "^1.1.1",
    "css-loader": "^5.0.0",
    "electron": "13.1.6",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.24.1",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.2",
    "webpack": "^5.44.0"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.0",
    "@fontsource/source-code-pro": "^4.5.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@sentry/node": "^6.9.0",
    "@sentry/react": "^6.9.0",
    "@sentry/tracing": "^6.9.0",
    "@timfish/webpack-asset-relocator-loader": "^0.1.0",
    "@types/base-64": "^1.0.0",
    "@types/card-validator": "^7.0.1",
    "@types/electron-store": "^3.2.0",
    "@types/graphql": "^14.5.0",
    "@types/keytar": "^4.4.2",
    "@types/luxon": "^1.27.1",
    "@types/overlayscrollbars": "^1.12.1",
    "@types/react-datepicker": "^4.1.5",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-table": "^7.7.2",
    "@types/react-tag-input": "^6.1.3",
    "@types/react-toastify": "^4.1.0",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.7",
    "@types/yup": "^0.29.13",
    "ajv": "^8.6.2",
    "ajv-formats": "^2.1.1",
    "apollo-link": "^1.2.14",
    "apollo-link-ws": "^1.0.20",
    "base-64": "^1.0.0",
    "card-validator": "^8.1.1",
    "copy-webpack-plugin": "^9.0.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.0",
    "file-loader": "^6.2.0",
    "graphql": "^15.5.1",
