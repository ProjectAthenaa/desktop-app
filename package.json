    "react-table": "^7.7.0",
    "react-tag-input": "^6.7.3",
    "react-text-transition": "^1.3.0",
    "react-toastify": "^8.0.1",
    "redux": "^4.1.1",
    "subscriptions-transport-ws": "^0.9.19",
    "uuid": "^8.3.2",
    "ws": "^8.2.0",
    "yup": "^0.32.9"
  }
}
